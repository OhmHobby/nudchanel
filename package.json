  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "generate:openapi": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit --runInBand -t swagger",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles --forceExit --ci --colors --coverage --collectCoverage --runInBand --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:it": "jest --config ./test/jest-integration.json --detectOpenHandles --forceExit --ci --colors --runInBand --reporters=default --reporters=jest-junit",
    "test:functional": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit --ci --colors --runInBand --reporters=default --reporters=jest-junit",
    "test:e2e": "cucumber-js",
    "test:cucumber:report": "cat results.json | cucumber-junit > junit.xml"
  },
