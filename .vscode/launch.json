  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "runtimeVersion": "20",
      "name": "Debug Nest",
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "program": "${workspaceRoot}/node_modules/@nestjs/cli/bin/nest.js",
      "args": ["start", "--watch"],
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development"
      },
      "restart": true
    }
  ]
}
